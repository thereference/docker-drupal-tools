{
    "name": "thereference/docker-drupal-tools",
    "description": "A set of tools that will be used for analyzing projects",
    "type": "project",
    "require": {
        "hirak/prestissimo": "^0.3.7",
        "behat/behat": "3.*",
        "behat/mink": "~1.7",
        "drush/drush": "^9.0",
        "behat/mink-extension": "^2.2",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "drupal/coder" : "^8.2",
